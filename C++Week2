#include <iostream>

#include <string>

using namespace std;

int main()

{

string pass = "search and penetrate";

string input;

cout << "12/5/1/13/3/8 1/17/4 15/5/17/5/11/13/1/11/5 \n Please decrypt the message above";

cin >> input;

if (input == pass)

{

cout << "You have now cracked the code. My bank details are 123456789";

}

else

{

cout << "You have failed terribly! Please try again. clue: Hello James = 8/5/19/19/6 10/1/18/5/12";

}

return 0;

}
